{
  "name": "python-action",
  "version": "1.0.0",
  "description": "A template to bootstrap the creation of a multi-platform Python GitHub action",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "ncc build index.js -o dist",
    "vendor-deps": "pip download -r src/requirements.txt --no-binary=:all: -d dist/vendor",
    "package": "npm run build && npm run vendor-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peter-evans/python-action.git"
  },
  "keywords": [],
  "author": "Peter Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peter-evans/python-action/issues"
  },
  "homepage": "https://github.com/peter-evans/python-action",
  "dependencies": {
    "@actions/core": "^1.1.1",
    "@actions/exec": "^1.0.3",
    "@actions/tool-cache": "^1.3.1"
  },
  "devDependencies": {
    "@zeit/ncc": "0.21.0"
  }
}
